code = "shader_type canvas_item;

void fragment()
{
	vec4 normal = texture(TEXTURE,UV);
	vec2 offset = normal.xy*2.0-vec2(1.0);
	COLOR.rgb = texture(SCREEN_TEXTURE,SCREEN_UV+offset*0.02).rgb;
	COLOR.a *= 0.75*normal.a;
}"

