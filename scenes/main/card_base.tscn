[sub_resource type="GDScript" id=24]

script/source = "extends Control


func _draw():
	for c in get_node(\"../ScrollContainer/VBoxContainer\").get_children():
		if (!has_node(\"../../Effects/\"+c.get_name())):
			continue
		
		var e = get_node(\"../../Effects/\"+c.get_name())
		var pos0 = (e.get_global_position()-get_global_position())/get_node(\"../../\").get_scale()/get_node(\"../../../\").get_scale()+e.get_size()/2
		var pos3 = (c.get_global_position()-get_global_position())/get_node(\"../../\").get_scale()/get_node(\"../../../\").get_scale()+Vector2(0,c.get_size().y/2)
		var pos2 = Vector2(200*sign(c.get_global_position().x-e.get_global_position().x)+get_size().x/2,pos3.y)
		draw_line(pos0,pos2,Color(0.2,0.4,0.8,0.5),8.0)
		draw_line(pos2,pos3,Color(0.2,0.4,0.8,0.5),8.0)

func _process(delta):
	update()

func _ready():
	update()
	set_process(true)
"

